{
  "name": "nuxt-my-template",
  "version": "1.0.0",
  "description": "My peachy Nuxt.js project",
  "author": "Yota Suzuki",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "fix": "eslint --fix",
    "host": "http-server ./dist -p 8000",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore ."
